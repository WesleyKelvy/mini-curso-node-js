<!DOCTYPE html>
<html lang="en">

<%- include('head.ejs') %>

<body>
    <div class="container">

        <div class="content">
            <%if (users.length> 0) {
                users.forEach(user => { %>
                <h3 class="title">
                    <%= user.firstName %>
                    <%= user.lastName %>
                </h3>
                <p>
                    <%= user.id %>
                </p>

                <%})}%>
        </div>

    </div>

</body>

</html>